# --- AWS Infrastructure & Identity ---

# Credentials
AWS_ACCESS_KEY_ID=AKIAABCDEFGH1234IJL5
AWS_DEFAULT_REGION=us-east-1
AWS_SECRET_ACCESS_KEY=abcdefghij

# The AWS CLI profile to use for deployment and execution
AWS_PROFILE=your_aws_profile_name

# Your 12-digit AWS Account ID (used for IAM policy ARNs)
ACCOUNT_ID=123456789012

# --- S3 & Lambda Configuration ---
# The name of the S3 bucket used for logs and forensics evidence
BUCKET_NAME=your-s3-bucket-name

# The S3 prefix (folder path) where forensics ISOs are dropped
PREFIX=evidence/forensics/

# The name of the AWS Lambda function that processes the malware
FUNCTION_NAME=MalwareForensicsProcessor

# --- Honeypot Server Configuration ---
# The IP address of your remote honeypot server (Cowrie/Vector)
SERVER_IP=192.168.x.x

# --- Blocklist Server Configuration ---
# The IP address of your remote blocklist server
BLOCKLIST_IP=137.184.21.255